page{
  height: 100%;
  width: 100%;
  background-color: #E3E0E0;
}

.head{
  margin-top: 4%; 
  height: 5%;
  background-color: #fff;
  font-size: 30rpx;
}
.ID{
  text-align: center;
  width: 32%;
  height: 100%;
  background-color: #7ad5f7;
  line-height: 60rpx;
  position: relative;
  margin-left: 6%;
  color: #fff;
  border-radius:30rpx;
}
.text{
  height: 65rpx;
  line-height: 65rpx;
  position: relative;
  left: 50%;
  top: -60rpx;
  color: #959595;
}

.time{
  text-align: center;
  width: 32%;
  height: 5%;
  background-color: #7ad5f7;
  line-height: 70rpx;
  position: relative;
  margin-left: 6%;
  margin-top: 3%;
  color: #fff;
  border-radius:30rpx;
  font-size: 30rpx;
}

.model{
  margin: 0 auto;
  margin-top: 30rpx;
  height: 230rpx;
  width: 90%;
  background-color: #fff;
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}

.act{
  height: 60rpx;
  line-height: 60rpx;
  background-color: #09B2F0;
  padding-left: 42rpx;
  color: #fff;
  border-top-left-radius:5px;
  border-top-right-radius:5px; 
  font-size: 30rpx;
}
.title{
  padding-left: 80rpx;
  font-size: 36rpx;
  padding-top: 12rpx;
  color: #303030;
}
.add{
  padding-left: 80rpx;
  padding-top: 12rpx;
  font-size: 30rpx;
  color: #959595;
}
.else{
  padding-left: 80rpx;
  font-size: 25rpx;
  padding-top: 12rpx;
  color: #959595;
}
.save{
  width: 600rpx;
  background-color: #09B2F0;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  color: #fff;
  font-size: 30rpx;
}